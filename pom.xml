	<profiles>
		<profile>
			<!-- 开发环境 -->
			<id>dev</id>
			<properties>
				<filters.env>dev</filters.env>
			</properties>
			<!-- 默认为开发环境,因为开发环境使用频繁 -->
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
		</profile>

		<profile>
			<!-- 生产环境 -->
			<id>product</id>
			<properties>
				<filters.env>product</filters.env>
			</properties>
		</profile>

	</profiles>

